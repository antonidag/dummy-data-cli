{
  "name": "ddcli",
  "version": "1.0.3",
  "description": "CLI tool for generating dummy data based on templates",
  "main": "ddcli.js",
  "bin": {
    "ddcli": "./ddcli.js"
  },
  "keywords": [
    "cli",
    "dummy",
    "template",
    "generator",
    "data",
    "json"
  ],
  "author": "Anton Bj√∂rkman",
  "license": "MIT",
  "homepage": "https://github.com/antonidag/dummy-data-cli",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/antonidag/dummy-data-cli.git"
  },
  "bugs": {
    "url": "https://github.com/antonidag/dummy-data-cli/issues"
  },
  "dependencies": {
    "@commitlint/cli": "^17.0.1",
    "@commitlint/config-conventional": "^17.0.0",
    "@sliit-foss/automatic-versioning": "^2.3.4",
    "cli-progress": "^3.12.0",
    "commander": "^9.0.0",
    "dummy-json": "^3.0.5",
    "husky": "^4.3.8",
    "inquirer": "^9.2.22",
    "jest": "^29.7.0",
    "yarn": "^1.22.22"
  },
  "type": "module",
  "scripts": {
    "test": "jest",
    "bump-version": "npx automatic-versioning --name=ddcli"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.24.5",
    "@babel/preset-react": "^7.24.1",
    "@babel/preset-typescript": "^7.24.1",
    "babel-jest": "^29.7.0"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "post-commit": "HUSKY_SKIP_HOOKS=1 yarn bump-version"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  }
}
